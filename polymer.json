{
	"entrypoint": "index.html",
	"shell": "src/my-app.html",
	"fragments": [
    "src/my-home.html",
    "src/my-projects.html",
    "src/my-about.html",
    "src/my-404.html",
    "src/my-shadow.html",
    "src/my-red.html",
    "src/my-pink.html",
    "src/my-purple.html",
    "src/my-deeppurple.html",
    "src/my-indigo.html",
    "src/my-blue.html",
    "src/my-lightblue.html",
    "src/my-cyan.html",
    "src/my-teal.html",
    "src/my-green.html",
    "src/my-lightgreen.html",
    "src/my-lime.html",
    "src/my-yellow.html",
    "src/my-amber.html",
    "src/my-orange.html",
    "src/my-deeporange.html",
    "src/my-brown.html",
    "src/my-grey.html",
    "src/my-bluegrey.html"
  ],
	"sources": [
    "src/**/*",
    "images/**/*",
    "data/**/*"
  ],
	"extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
	"lint": {
		"rules": ["polymer-2"]
	},
	"builds": [{
		"bundle": true,
		"addServiceWorker": true,
		"js": {
			"compile": true,
			"minify": true
		},
		"css": {
			"minify": true
		},
		"html": {
			"minify": true
		}
	}]
}
